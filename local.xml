<project name="local">
  <!-- = Project configuration =============================== -->
  <!-- ======================================================= -->
  <!-- Path to the Java core libraries.                        -->
  <property name="java-lib" location="/System/Library/Frameworks/JavaVM.framework/Versions/1.4.2/Classes/classes.jar"/>

  <!-- Path to the ProGuard binaries.                          -->
  <!-- Defaults to the one bundled with the project, but can   -->
  <!-- be changed to another version by editing this value.    -->
  <property name="proguard" location="${tools}/proguard.jar"/>

  <!-- Path to the Ant core libraries (used to compile tools). -->
  <property name="ant-lib"  location="/usr/local/ant/lib/ant.jar"/>
</project>
